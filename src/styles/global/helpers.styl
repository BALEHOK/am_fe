.hide-text
  font 0/0 a
  color transparent
  text-shadow none
  background-color transparent
  border 0

.hide
  display none

.invisible
  visibility hidden

.pull-left
  float left

.pull-right
  float right

.txt-left
  text-align left

.txt-center
  text-align center

.txt-right
  text-align right
